{
    "user": "blefnk",
    "repo": "streamer",
    "resources": [
        {
            "name": "^streamer-(.*).zip$",
            "platform": "linux",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/streamer.so"]
        },
        {
            "name": "^streamer-(.*).zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/streamer.dll"]
        }
    ],
    "runtime": {
        "plugins": ["blefnk/streamer"]
    }
}
